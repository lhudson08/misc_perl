dlog <- function(alpha = 0.01, rd=1, NO=2, t=15){#
	N <- c(NO, numeric(t))#
	for (i in 1:t) N[i + 1] <- {#
		N[i] + rd * N[i] * (1 - alpha * N[i])#
	}#
	return(N)#
}
